config MTK_ION_CACHE_OPTIMIZATION
	bool "MTK FPGA EARLY PORTING"
	default n
	help
	  This option is used for optimize ion cache
	  operation flow, users only pass flushed address
	  and range, CMO driver decides using which operations
	  (by set/way or by range).

config MTK_CACHE_FLUSH_BY_SF
	bool "cache flush by mtk SF"
	default n
