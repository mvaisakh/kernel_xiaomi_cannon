&i2c3 {
	#address-cells = <1>;
	#size-cells = <0>;
	rt9759_slave@66 {
		compatible = "richtek,rt9759";
		status = "okay";
		reg = <0x66>;
		rm_name = "rt9759";
		rm_slave_addr = /bits/ 8 <0x66>;
		rt9759,intr-gpios = <&pio 22 0x0>;
		standalone {
			chg_name = "primary_divider_chg";
			vbatovp = <4500000>;
