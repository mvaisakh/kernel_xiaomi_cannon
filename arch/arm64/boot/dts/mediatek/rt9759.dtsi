			ibatocp = <6100000>;
			ibatocp_alm = <6000000>;
			ibatucp_alm = <1000000>;
			vbusovp = <10000000>;
			vbusovp_alm = <9000000>;
			ibusocp = <4250000>;
			ibusocp_alm = <4000000>;
			vacovp = <11000000>;
			wdt = <30000000>;
			ibat_rsense = <1>; /* 0: 2mohm, 1: 5mohm */
